<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="reference|dimension" />
        <attr name="roundHeight" format="reference|dimension" />
        <attr name="sideWidth" format="reference|dimension" />
        <attr name="sideColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="TopBar">
        <attr name="L1src" format="reference|color" />
        <attr name="L2src" format="reference|color" />
        <attr name="R1src" format="reference|color" />
        <attr name="CSrc" format="reference|color" />
